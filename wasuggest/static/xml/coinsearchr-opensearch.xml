<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/"
                       xmlns:moz="http://www.mozilla.org/2006/browser/search/">
  <ShortName>WolframAlphaSuggest</ShortName>
  <Description>WolframAlphaSuggest - results/suggestions from WolframAlpha</Description>
  <InputEncoding>[UTF-8]</InputEncoding>
  <Image width="32" height="32" type="image/png">https://wasuggest.coinsearchr.com/static/img/favicon-32x32.png</Image>
  <Image width="16" height="16" type="image/png">https://wasuggest.coinsearchr.com/static/img/favicon-16x16.png</Image>
  <Image width="16" height="16" type="image/x-icon">https://wasuggest.coinsearchr.com/favicon.ico</Image>
  <Url type="text/html" template="https://wasuggest.coinsearchr.com/search/">
    <Param name="q" value="{searchTerms}"/>
  </Url>
  <Url type="application/x-suggestions+json" rel="suggestions" template="https://wasuggest.coinsearchr.com/suggest">
    <Param name="q" value="{searchTerms}"/>
  </Url>
  <moz:SearchForm>https://wasuggest.coinsearchr.com/</moz:SearchForm>
</OpenSearchDescription>